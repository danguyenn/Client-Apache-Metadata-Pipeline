from superset import security_manager
print([u.username for u in security_manager.get_all_users()])
# You should see ['admin', ...]
exit()
from superset import db, security_manager
user = security_manager.find_user(username="admin")
security_manager.reset_password(user, "admin")
user = security_manager.find_user(username="admin")
security_manager.reset_password(user.id, "admin")
db.session.commit()
exit()
